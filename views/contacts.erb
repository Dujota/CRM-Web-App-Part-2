<h1>Contacts</h1>

<section class="count">
  There are <span> <%= @contacts.length%> </span> contacts
</section>
<!-- List out contact information  -->

<section class="contact">
  <ul>
    <% @contacts.each do |contact| %>
      <li>
        <%= " ID: #{contact.id} Name: #{contact.full_name} Email:#{contact.email}" %>
        <aside>
          <a href="#">Edit</a>|<a href="#">Delete</a>
        </aside>
      </li>
    <% end %>
  </section>
</ul>
